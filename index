    // Function to get subject icon
    function getSubjectIcon(subject) {
      const icons = {
        'Matematika': 'â—',
        'Matematika TL': 'ğŸ“',
        'Bahasa Indonesia': 'ğŸ“–',
        'Bahasa Inggris': 'ğŸ—£ï¸',
        'Bahasa Sunda': 'ğŸ—ºï¸',
        'Fisika': 'âš›ï¸',
        'Kimia': 'ğŸ§ª',
        'Biologi': 'ğŸ§¬',
        'Geografi': 'ğŸŒ',
        'Sejarah': 'ğŸ“œ',
        'Sosiologi': 'ï¿½ï¿½ï¿½',
        'Ekonomi': 'ğŸ’°',
        'PJOK': 'âš½',
        'BK': 'ğŸ¯',
        'Informatika': 'ğŸ’»',
        'Seni Budaya': 'ğŸ¨',
        'Prakarya': 'ğŸ”¨',
        'Pendidikan Pancasila': 'ğŸ‡®ğŸ‡©',
        'PAI': 'â˜ªï¸',
        'Pendidikan Agama Islam dan Budi Pekerti': 'â˜ªï¸',
        'PAI dan Budi Pekerti': 'â˜ªï¸',
        'Sosiologi / Bahasa Sunda': 'ğŸ‘¥'
      };
      return icons[subject] || 'ğŸ“š';
    }

    // Function to create teacher card
    function createTeacherCard(teacher) {
      const card = document.createElement('div');
      card.className = 'teacher-card bg-white rounded-xl shadow-lg overflow-hidden card-hover fade-in';
      card.setAttribute('data-category', teacher.category);
      
      card.innerHTML = `
        <div class="p-6">
          <div class="flex flex-col items-center text-center">
            <div class="rounded-full overflow-hidden border-4 border-green-300 shadow-md mb-4" style="width: 140px; height: 140px;">
              <img 
                src="${teacher.photo}" 
                alt="${teacher.name}" 
                class="w-full h-full object-cover object-center"
                onerror="this.style.display='none'; this.parentElement.innerHTML='<div style=\\'display:flex;align-items:center;justify-content:center;height:100%;font-size:4rem;color:#10b981;background:#f0fdf4;\\'>${getSubjectIcon(teacher.subject)}</div>';"
              >
            </div>
            <div class="inline-flex items-center gap-2 px-3 py-1 bg-green-100 text-green-700 text-xs font-semibold rounded-full mb-3">
              <span>${getSubjectIcon(teacher.subject)}</span>
              <span>${teacher.subject}</span>
            </div>
            <h4 class="text-base font-bold mb-1" style="color: #059669;">${teacher.name}</h4>
          </div>
        </div>
      `;
      
      return card;
    }

    // Function to render teachers
    function renderTeachers(filter = 'all') {
      const grid = document.getElementById('teachers-grid');
      grid.innerHTML = '';
      
      const filteredTeachers = filter === 'all' 
        ? teachersData 
        : teachersData.filter(t => t.category === filter);
      
      filteredTeachers.forEach(teacher => {
        grid.appendChild(createTeacherCard(teacher));
      });
    }

    // Filter functionality
    const filterButtons = document.querySelectorAll('.filter-btn');
    filterButtons.forEach(btn => {
      btn.addEventListener('click', function() {
        // Update active button
        filterButtons.forEach(b => {
          b.classList.remove('active');
          b.style.background = 'white';
          b.style.color = '#059669';
        });
        this.classList.add('active');
        this.style.background = '#059669';
        this.style.color = 'white';
        
        // Filter teachers
        const filter = this.getAttribute('data-filter');
        renderTeachers(filter);
      });
    });

    // Initial render
    renderTeachers();
